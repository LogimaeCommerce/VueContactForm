\n          </div>\n          <div class=\"col-1 m-auto p-0 bars-container\">\n            <font-awesome-icon v-if=\"tableData.length > 1\" icon=\"bars\" class=\"bars fa-md\"/>\n          </div>\n        </div>\n      </transition-group>\n    </draggable>\n    <hr/>\n    <a class=\"link\" @click=\"addRow()\">\n      <font-awesome-icon class=\"text-success\" icon=\"plus-circle\"/>\n      Add Row\n    </a>\n  </section>\n</template>\n\n<script>\nimport draggable from 'vuedraggable'\n\nexport default {\n  components: {\n    draggable\n  },\n  data () {\n    return {\n      drag: null,\n      tableData: [\n        {\n          columnA: 'a',\n          columnB: 'b',\n          columnC: 'c',\n          columnD: 'd'\n        },\n        {\n          columnA: 'a',\n          columnB: 'b',\n          columnC: 'c',\n          columnD: 'd'\n        },\n        {\n          columnA: 'a',\n          columnB: 'b',\n          columnC: 'c',\n          columnD: 'd'\n        }\n      ]\n    }\n  },\n  methods: {\n    addRow () {\n      this.tableData.push({\n        columnA: '',\n        columnB: '',\n        columnC: '',\n        columnD: ''\n      })\n    },\n    removeRow (deletedIndex) {\n      this.tableData = this.tableData.filter((row, index) => {\n        return index !== deletedIndex\n      })\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.action-row {\n  width: 25px;\n}\n\n.bars-container {\n  cursor: move;\n}\n\n.add-column {\n  svg {\n    color: green;\n  }\n}\n\n.link {\n  cursor: pointer;\n}\n</style>\n"]}]}